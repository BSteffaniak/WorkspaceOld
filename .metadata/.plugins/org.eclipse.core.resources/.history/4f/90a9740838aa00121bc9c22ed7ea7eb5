package net.foxycorndog.tetris.pieces;

public class Piece
{
	public Piece(float matrix[], int width)
	{
		int height = matrix.length / width;
		
		for (int x = 0; x < width; x++)
		{
			for (int y = 0; y < height; y++)
			{
				if (matrix[x + y * width] >= 0)
				{
					
				}
			}
		}
	}
	
	public void render()
	{
		
	}
}