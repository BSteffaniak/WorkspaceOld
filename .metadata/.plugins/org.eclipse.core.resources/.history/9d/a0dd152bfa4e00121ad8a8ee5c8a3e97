package net.foxycorndog.arrowide.tabmenu;

import org.eclipse.swt.widgets.TabItem;

public class TabMenu
{
	private class Tab
	{
		private String  fileLocation;
		
		private TabItem tab;
		
		public Tab(TabItem tab, String fileLocation)
		{
			this.tab          = tab;
			
			this.fileLocation = fileLocation;
		}
	}
}