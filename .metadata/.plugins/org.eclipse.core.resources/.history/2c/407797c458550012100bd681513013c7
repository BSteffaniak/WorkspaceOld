		GL_MULTITEXCOORD5.setColor(AssemblyLanguage.KEYWORD_COLOR);
		GL_MULTITEXCOORD6.setColor(AssemblyLanguage.KEYWORD_COLOR);
		GL_MULTITEXCOORD7.setColor(AssemblyLanguage.KEYWORD_COLOR);
