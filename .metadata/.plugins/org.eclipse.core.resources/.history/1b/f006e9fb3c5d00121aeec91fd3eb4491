package net.foxycorndog.arrowide.components.menubar;

import org.eclipse.swt.SWT;
import org.eclipse.swt.widgets.Shell;

public class DropdownMenu
{
	private Shell shell;
	
	public DropdownMenu()
	{
		shell = new Shell(SWT.NO_TRIM);
	}
	
	public void
	
	public void open()
	{
		shell.open();
	}
	
	public void close()
	{
		shell.close();
	}
	
	public void dispose()
	{
		shell.dispose();
	}
}