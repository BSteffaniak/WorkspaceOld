						if (!fileCacheSaved.get(loc))
						{
							String result = null;
							
							OptionDialog optDialog = new OptionDialog("Save?", "Would you like to save before renaming?");
							
							result = optDialog.open();
							
							if (result != null)
							{
								if (result.equals("yes"))
