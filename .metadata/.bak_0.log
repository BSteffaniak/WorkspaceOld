									saveFile(loc);
									
									willContinue	= true;
								}
								else if (result.equals("no"))
								{
									willContinue = true;
								}
							}
						}
						else
						{
							willContinue = true;
						}
					}
					else
					{
						willContinue = true;
					}
					
