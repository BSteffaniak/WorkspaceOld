			PROPERTIES.put("os.name", "windows");
			PROPERTIES.put("composite.modifiers", SWT.NONE);
			PROPERTIES.put("key.control", SWT.CTRL);
			PROPERTIES.put("os.executable.extension", ".exe");
			PROPERTIES.put("colon", ';');
