				if (outputs[0].getResult() == 0)
				{
					display.syncExec(new Runnable()
					{
						public void run()
						{
							if (stream != null)
							{
								stream.println("Compiled successfully.");
							}
						}
					});
					
					if (isInViewer)
					{
						DISPLAY.syncExec(new Runnable()
						{
							public void run()
							{
								for (int i = 0; i < files.length; i ++)
								{
									addToFileViewer(files[i]);
								}
							}
						});
					}
				}
				
				DISPLAY.syncExec(new Runnable()
				{
					public void run()
					{
						for (int i = 0; i < outputs.length; i++)
						{
							if (outputs[i].getResult() != 0)
